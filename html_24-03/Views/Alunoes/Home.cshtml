@model IEnumerable<html_24_03.Models.Aluno>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.alunoNome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.alunoCurso)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.alunoNome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.alunoCurso.cursosId)
            </td>
        </tr>
}
    </tbody>
</table>
