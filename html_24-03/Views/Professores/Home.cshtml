@model IEnumerable<html_24_03.Models.Professores>

@{
    ViewData["Title"] = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Home</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProfessoresNome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProfessoresCurso)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProfessoresNome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProfessoresCurso.cursosId)
            </td>
        </tr>
}
    </tbody>
</table>
